#!/bin/bash

exec 2>&1
set -euo pipefail
IFS=$'\n\t'

RET=$(get-private-ppa ppa:oem-solutions-group/ppa)

if [ "$(echo "$RET" | awk '{print $1}')" != "oem-solutions-group-ubuntu-ppa" ]; then
    exit 1
fi

if [ "$(echo "$RET" | awk '{print $3}')" != "7B85BB3BB81D9DAFF9F06250B52B913A41086767" ]; then
    exit 1
fi

exit
